{
  "short_name": "Stereo",
  "id": "stereo-viewer",
  "name": "Stereo Viewer",
  "description": "View stereo 3D pictures on desktop and mobile, with VR support",
  "icons": [
    {
      "src": "icons/icon.svg",
      "sizes": "any",
      "type": "image/svg+xml"
    },
    {
      "src": "icons/icon.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "icons/icon-maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "sizes": "1080x2340",
      "src": "/screenshots/screenshot-empty.png",
      "type": "image/png"
    },
    {
      "sizes": "1080x2340",
      "src": "/screenshots/screenshot-demo.png",
      "type": "image/png"
    }
  ],
  "start_url": "/",
  "display": "standalone",
  "scope": "/",
  "theme_color": "#00ff8f",
  "background_color": "#000000",
  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "files",
          "accept": ["image/*"]
        }
      ]
    }
  }
}